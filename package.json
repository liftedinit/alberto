    "@liftedinit/ui": "github:liftedinit/lifted-ui",
    "@tanstack/react-query": "^4.36.1",
    "@web3auth/auth-adapter": "^9.7.0",
    "@web3auth/base": "^9.7.0",
    "@web3auth/base-provider": "^9.7.0",
    "@web3auth/modal": "^9.7.0",
    "big.js": "^6.2.1",
    "buffer": "^6.0.3",
